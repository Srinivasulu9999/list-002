# TO PERFORM LEFT-ROTATE -BY-two
def left_rotate(inputlist):
    if len(inputlist)<2:
        return inputlist
    else:
        return inputlist[2:]+inputlist[:2]
original_list =[1,2,3,4]
result = left_rotate(original_list)
print(original_list)
print("left rotated:",result)

def right_rotate(input):
    if len(input)<2:
        return input
    else:
        return input[-2:]+input[:-2]
original_list=[1,2,3,4,5,6,7,8]
result= right_rotate(original_list)
print(original_list)
print(result)

#FIND TOP MAXIMUM NUMBERS
def top_two(numbers):
    max1=float(input("Enter the string:"))
    max2=float(input("Enter the string:"))
    for num in numbers:
        if num >max1:
            max2=max1
            max1=num
        elif num >max2:
            max2 = num
    return max1,max2

numbers=[10,5,15,7,20,25]
max1,max2 =top_two(numbers)
print("max1:",max1)
print("max2:",max2)

