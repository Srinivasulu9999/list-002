def replace_vowels(input_str):
    vowels='aeiouAEIOU'
    for chr in input_str:
        if chr in vowels:
            return input_str.replace(chr,'_',1)
givenstring='replace vowels'
cc=replace_vowels(givenstring)
print(cc)









