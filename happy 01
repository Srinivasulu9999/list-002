def top_two(numbers):
    max1=int(input("Enter the number:"))
    max2=int(input("Enter the number:"))
    for num in numbers:
        if num>max1:
            max1=max2
            max1=num
        elif num>max2:
            max2=num
    return max1,max2
numbers=[11,22,33,344,7654]
print(top_two(numbers))

def is_perfectnumber(n):
    if n<0:
        return False
    divisor_num=0
    for i in range(1,n):
        if n%i ==0:
            divisor_num +=i
    return divisor_num ==n
num=28
if is_perfectnumber(num):
    print("is a perfect number:")
else:
    print("is not a perfect number:")
